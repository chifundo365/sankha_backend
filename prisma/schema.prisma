model bulk_uploads {
  id            String                @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  shop_id       String                @db.Uuid
  file_name     String                @db.VarChar(255)
  total_rows    Int
  successful    Int                   @default(0)
  failed        Int                   @default(0)
