  @@index([batch_id], map: "idx_staging_batch_id")
  @@index([shop_id, batch_id], map: "idx_staging_shop_batch")
  @@index([validation_status], map: "idx_staging_validation_status")
  @@index([bulk_upload_id], map: "idx_staging_bulk_upload_id")
